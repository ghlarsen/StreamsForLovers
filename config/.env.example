# Interactive AI Music Streaming - Environment Variables
# Copy this file to .env and fill in your actual API keys

# YouTube API Configuration
YOUTUBE_API_KEY=your_youtube_data_api_key_here
YOUTUBE_CHANNEL_ID=your_youtube_channel_id_here
YOUTUBE_STREAM_KEY=your_youtube_live_stream_key_here

# Suno AI Music Generation API
SUNO_API_KEY=your_suno_api_key_here
SUNO_API_URL=https://sunoapi.com/api/v1
SUNO_API_TIMEOUT=60

# Adobe Firefly Video Generation API
ADOBE_FIREFLY_API_KEY=your_adobe_firefly_api_key_here
ADOBE_FIREFLY_API_URL=https://firefly-api.adobe.io/v2
ADOBE_FIREFLY_API_TIMEOUT=120

# OBS Studio Configuration
OBS_WEBSOCKET_HOST=localhost
OBS_WEBSOCKET_PORT=4444
OBS_WEBSOCKET_PASSWORD=your_obs_websocket_password

# Stream Configuration
STREAM_TITLE="Interactive AI Music Stream ðŸŽµ"
STREAM_DESCRIPTION="24/7 AI-generated music that responds to your chat!"
RTMP_SERVER=rtmp://a.rtmp.youtube.com/live2/

# Optional: Weather Integration
WEATHER_API_KEY=your_openweather_api_key_here
WEATHER_CITY=tokyo

# Application Settings
LOG_LEVEL=INFO
MAX_QUEUE_SIZE=50
GENERATION_INTERVAL_MINUTES=30
CHAT_POLL_INTERVAL_SECONDS=2

# Music Generation Limits
MAX_DAILY_GENERATIONS=100
COST_LIMIT_DAILY_USD=5.00

# Development/Debug Settings
DEBUG_MODE=false
MOCK_CHAT_MESSAGES=false
SKIP_MUSIC_GENERATION=false